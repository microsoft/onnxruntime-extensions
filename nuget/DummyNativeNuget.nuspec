    <readme>README.md</readme>
    <projectUrl>https://github.com/Microsoft/onnxruntime-extensions</projectUrl>
    <repository type="git" url="https://github.com/Microsoft/onnxruntime-extensions.git" commit="COMMIT_ID" />
  </metadata>
